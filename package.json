{
  "name": "create-exposed-app",
  "description": "App generator with everything exposed for maximum tweaking",
  "license": "MIT",
  "author": "Matt Turnbull <matt@iamturns.com> (https://iamturns.com)",
  "homepage": "https://github.com/iamturns/create-exposed-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamturns/create-exposed-app.git"
  },
  "bugs": {
    "url": "https://github.com/iamturns/create-exposed-app/issues"
  },
  "version": "0.0.0-development",
  "main": "dist/create-exposed-app.js",
  "scripts": {
    "pre-commit": "lint-staged && npm run build",
    "build": "run-p --print-label build:src build:dts",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:src": "babel src --out-dir dist --extensions '.ts,.tsx' --source-maps",
    "build:watch": "onchange --initial --kill --delay 1000 'src/**/*' -- npm run build",
    "dev": "onchange --initial --kill --delay 1000 'src/**/*' -- npm run dev:onchange",
    "dev:onchange": "npm run build && npm run start:dev",
    "format": "run-s format:developing format:package format:imports format:prettier format:eslint",
    "format:developing": "doctoc --maxlevel 3 --notitle ./DEVELOPING.md",
    "format:eslint": "eslint --cache --ext '.js,.jsx,.ts,.tsx' --fix ./ >/dev/null 2>&1 || true",
    "format:imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "format:package": "prettier-package-json --write",
    "format:prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}' '.editorconfig' 'LICENSE'",
    "lint": "eslint --cache --ext '.js,.jsx,.ts,.tsx' ./",
    "prepublish": "rimraf dist && npm run build",
    "reinstall": "iamturns-scripts reinstall",
    "reset": "iamturns-scripts reset",
    "start": "node dist/create-exposed-app.js",
    "start:dev": "NODE_ENV=development npm run start",
    "test": "jest --passWithNoTests --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run type-check -- --watch",
    "upgrade": "iamturns-scripts upgrade",
    "validate": "run-p --print-label lint typecheck test build"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cross-spawn": "^6.0.5",
    "debug": "^4.1.0",
    "ejs": "^2.6.1",
    "eslint": "^5.5.0",
    "inquirer": "^6.2.1",
    "npm-check": "^5.8.0",
    "prettier": "^1.14.3",
    "read-pkg-up": "^4.0.0",
    "recursive-copy": "^2.0.9",
    "rimraf": "^2.6.2",
    "yargs-parser": "^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/debug": "0.0.31",
    "@types/ejs": "^2.6.1",
    "@types/inquirer": "0.0.43",
    "babel-jest": "^23.6.0",
    "doctoc": "^1.3.1",
    "eslint-config-iamturns": "^1.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^1.1.1",
    "iamturns-scripts": "^5.0.0",
    "import-sort-cli": "^5.2.0",
    "import-sort-parser-babylon": "5.2.0",
    "import-sort-parser-typescript": "5.0.0",
    "import-sort-style-module": "5.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "prettier-package-json": "^2.0.1",
    "typescript": "~3.1.1",
    "typescript-eslint-parser": "^21.0.2"
  }
}
